extern func printf(s:str, ...)

func main -> int
    int x = 4
    
    int a1 = x & 5
    int a2 = x | 5
    int a3 = x ^ 5
    int a4 = x << 2
    int a5 = x >> 2
    
    printf("x & 5 = %x\n", a1)
    printf("x | 5 = %x\n", a2)
    printf("x ^ 5 = %x\n", a3)
    printf("x << 2 = %x\n", a4)
    printf("x >> 2 = %x\n", a5)
    
    return 0
end

