
extern printf(s:str, ...)

func add_two(x:int, y:int) -> int
    int answer = x + y
    printf("Answer: %d\n", answer)
    return answer
end

func main -> int
    int answer1 = add_two(10, 5)
    int answer2 = add_two(6, 3) * 2
    
    printf("Answer1: %d\n", answer1)
    printf("Answer2: %d\n", answer2)
    
    return 0
end
